    834          /******** 发送一个字节的数据到液晶控制器*************************/
    835          static void SendByte(unsigned char Data)
    836          {
    837            unsigned char i;
    838            LCD_CE=0;
    839            for(i=0;i<8;i++)
    840            {
    841              LCD_SCL = 0;
    842              if(Data & 0x80)
    843              {
    844                LCD_SDA = 1;
    845              }
    846              else LCD_SDA = 0;
    847              LCD_SCL = 1;
    848              Data<<=1;
    849            }
    850            LCD_CE=1;
    851          }
