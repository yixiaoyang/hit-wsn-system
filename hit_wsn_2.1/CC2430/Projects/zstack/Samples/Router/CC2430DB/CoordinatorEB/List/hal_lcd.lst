    853          /*-----------------------------------------------------------------------
    854          LCD_write_byte    : 使用SPI接口写数据到LCD
    855          
    856          输入参数：data    ：写入的数据；
    857          command ：写数据/命令选择；
    858          
    859          编写日期          ：2004-8-10
    860          最后修改日期      ：2004-8-13
    861          -----------------------------------------------------------------------*/
    862          static void LCD_write_byte(unsigned char data, unsigned char command)
    863          {
    864            LCD_CE = 1 ;		 // 使能LCD
    865          
    866            if (command == 0)
    867              LCD_DC = 0;	        // 传送命令
    868            else
    869              LCD_DC = 1;		// 传送数据
    870            SendByte(data);
    871            LCD_CE = 0;	        // 关闭LCD
    872            LCD_DC = 1;	
    873          }
