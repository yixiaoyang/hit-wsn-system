    924          /*-----------------------------------------------------------------------
    925          LCD_init          : 3310LCD初始化
    926          
    927          编写日期          ：2004-8-10
    928          最后修改日期      ：2004-8-10
    929          -----------------------------------------------------------------------*/
    930          static void initLcd(void)
    931          {
    932            // CC2430 中，I/O 口做普通 I/O 使用时和每个 I/O 端口相关的寄存器有 3 个，分别是 //PxSEL
    933            //功能选择寄存器，PxDIR 方向寄存器，PxINP 输入模式寄存器，其中 x 为 0，1，2 。
    934          
    935            P1SEL &= 0x0F; //P1高4位 为普通 I/O 口
    936            P1DIR |= 0xF0; //P1高4位 设置为输出
    937          
    938            LCD_CE = 0;    // 关闭LCD
    939            Delay_nms(1);
    940            LCD_CE = 1;    // 使能LCD
    941            Delay_nms(1);
    942          
    943            LCD_write_byte(0x21, 0);	// 使用扩展命令设置LCD模式
    944            LCD_write_byte(0xc8, 0);	// 设置偏置电压
    945            LCD_write_byte(0x06, 0);	// 温度校正
    946            LCD_write_byte(0x13, 0);	// 1:48
    947            LCD_write_byte(0x20, 0);	// 使用基本命令
    948            LCD_clear();	                // 清屏
    949            LCD_write_byte(0x0c, 0);	// 设定显示模式，正常显示
    950            LCD_CE=0 ;                    // 关闭LCD
    951          }
    952          
    953          
    954          /*-----------------------------------------------------------------------
    955          LCD_write_english_String  : 英文字符串显示函数
    956          
    957          输入参数：*s      ：英文字符串指针；
    958          X、Y    : 显示字符串的位置,x 0-83 ,y 0-5
    959          
    960          编写日期          ：2004-8-10
    961          最后修改日期      ：2004-8-10 		
    962          -----------------------------------------------------------------------*/
    963          static void LCD_write_english_string(unsigned char X,unsigned char Y,uint8 *s)
    964          {
    965            unsigned char i=0;
    966            LCD_set_XY(0,Y);//把第Y行清零
    967            for(i=0;i<HAL_LCD_MAX_CHARS*6;i++)
    968            {
    969              LCD_write_byte(0,1);
    970            }
    971            LCD_set_XY(X,Y);
    972            i=0;
    973            while (*s)
    974            {
    975              LCD_write_char(*s);
    976              s++;
    977              i++;
    978              if(i>=HAL_LCD_MAX_CHARS)return;
    979            }
    980          }
